#
msgid "Your is administrator's tenant"
msgstr "Bạn là quản trị viên tổ chức"

#
msgid "Your isn't administrator's tenant"
msgstr "Bạn không phải là quản trị viên tổ chức"

#
msgid "Two-Factor Authentication"
msgstr "Xác thực 2 bước"

#
msgid "Integrated"
msgstr "Đã tích hợp"

#
msgid "Integrate now"
msgstr "Tích hợp ngay"

#
msgid "Enable"
msgstr "Kích hoạt"

#
msgid "Disable"
msgstr "Huỷ kích hoạt"

#
msgid "or enter the code manually"
msgstr "hoặc nhập mã"

#
msgid "Step 1"
msgstr "Bước 1"

#
msgid "Step 2"
msgstr "Bước 2"

#
msgid "Scan the QR code using your 2FA app, or manually enter the provided setup key."
msgstr "Quét mã QR bằng ứng dụng 2FA của bạn hoặc nhập thủ công khóa thiết lập được cung cấp."

#
msgid "Enter the one-time code (OTP) generated by your 2FA app into the provided field."
msgstr "Nhập mã một lần (OTP) do ứng dụng 2FA của bạn tạo vào trường được cung cấp."

#
msgid "Click Verify to confirm."
msgstr "Nhấn nút Xác minh để xác nhận."

#
msgid "2FA will be activated upon successful integration. The system may require you to log in again."
msgstr "2FA sẽ được kích hoạt ngay sau khi tích hợp thành công. Hệ thống có thể yêu cầu bạn đăng nhập lại."

#
msgid "Integrate status"
msgstr "Trạng thái tích hợp"

#
msgid "The OTP code is required"
msgstr "Mã OTP là bắt buộc"

#
msgid "The system only provides one 2FA configuration per user."
msgstr "Hệ thống chỉ cung cấp cho mỗi người một cấu hình 2FA."

#
msgid "All actions performed with the 2FA setting will require OTP entry."
msgstr "Mọi thao tác thực hiện với cài đặt 2FA sẽ bắt buộc nhập OTP."

#
msgid "Enabling 2FA will require OTP verification after login and sensitive operations."
msgstr "Kích hoạt 2FA sẽ bắt buộc nhập xác thực OTP sau khi đăng nhập và các thao tác nhạy cảm."

#
msgid "For any issues related to 2FA, please contact the administrator for resolution."
msgstr "Mọi vấn đề liên quan đến 2FA vui lòng liên hệ quản trị viên để giải quyết."

#
msgid "OTP is only valid once per cycle and reuse is not accepted in cycle."
msgstr "OTP chỉ có hiệu lực một lần trong mỗi chu kỳ và không chấp nhận sử dụng lại trong chu kỳ."

#
msgid "Enter the OTP verification code"
msgstr "Nhập mã xác thực OTP"

#
msgid "Enter verification code"
msgstr "Nhập mã xác thực"

#
msgid "2FA Authentication"
msgstr "Xác thực Hai Yếu Tố (2FA)"

#
msgid "Or another application that supports 2FA with a token"
msgstr "Hoặc một ứng dụng khác hỗ trợ 2FA bằng mã token."

#
msgid "Application supports 2FA"
msgstr "Ứng dụng hỗ trợ 2FA"

#
msgid "Install application at"
msgstr "Cài đặt ứng dụng tại"

#
msgid "My profile"
msgstr "Hồ sơ của tôi"
